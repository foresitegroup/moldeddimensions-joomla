}
.add-nav li:first-child{padding-left:0;}
.add-nav li:first-child:before{display:none;}
.add-nav a{
	text-decoration:none;
	color:#5a5a5a;
}
.add-nav a:hover{text-decoration:underline;}
.social-network{
