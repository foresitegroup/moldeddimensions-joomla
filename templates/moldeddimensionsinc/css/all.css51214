/* main container styles */
#wrapper{
	width:100%;
	overflow:hidden;
	position:relative;
}

/* header styles */
#header{
	padding:5px 0 0;
	border-top:5px solid #c9201c;
	border-bottom:3px solid #cbc9c9;
