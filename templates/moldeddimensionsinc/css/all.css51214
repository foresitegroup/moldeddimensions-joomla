	font:400 14px/16px 'lato', sans-serif;
	color:#5a5a5a;
}
.nav-holder{overflow:hidden;}
.nav-holder .slide{position:static !important;}
.nav-holder .opener{
	display:none;
	position:absolute;
	top:0;
	right:0;
	width:49px;
	height:44px;
	text-indent:-9999px;
	overflow:hidden;
	background:url(../images/sprite.png) no-repeat 0 -112px;
}
.mobile-heading{
	display:none;
	padding:10px 60px 10px 17px;
	margin:0;
	background:#c91d1b;
	font:400 21px/24px 'lato', sans-serif;
}
.mobile-heading a{
	text-decoration:none;
	color:#fff;
}
.mobile-heading a:hover{text-decoration:underline;}
.drop-slide{display:none;}
#nav .add-slide .open{
	display:none;
	position:absolute;
	top:0;
	right:0;
	width:40px;
	height:42px;
	padding:0;
	text-indent:-9999px;
	overflow:hidden;
	border-left:1px solid #353535;
	background:#1e1e1e;
}
.add-slide .open:after{
	content:'';
	position:absolute;
	top:19px;
	left:16px;
	width:9px;
	height:5px;
	background:url(../images/sprite.png) no-repeat -57px -112px;
}
