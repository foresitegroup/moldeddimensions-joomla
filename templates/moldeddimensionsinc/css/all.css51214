}
.add-nav ul{
	overflow:hidden;
	list-style:none;
	margin:0;
	padding:0;
}
.add-nav li{
	float:left;
	list-style:none outside;
	padding:0 0 0 5px;
}
.add-nav li:before{
    float:left;
    content:'\002f';
